{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/Welcome",
    "definitions": {
        "Welcome": {
            "type": "array",
            "additionalProperties": false,
            "properties": {
                "datasets": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Dataset"
                    }
                },
                "labels": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                }
            },
            "required": [
                "datasets",
                "labels"
            ],
            "title": "Welcome"
        },
        "Dataset": {
            "type": "array",
            "additionalProperties": false,
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "backgroundColor": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "label": {
                    "type": "string"
                }
            },
            "required": [
                "backgroundColor",
                "data",
                "label"
            ],
            "title": "Dataset"
        }
    }
}

